{
  "name": "@truenine/agents-cli",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@10.18.2",
  "description": "Agent CLI Prompt Management Solution - Initialize and manage .ai/ prompt repositories",
  "author": "truenine",
  "license": "LGPL-3.0-or-later",
  "keywords": [
    "cli",
    "agents",
    "prompts",
    "ai",
    "claude",
    "template",
    "management"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-cli": "dist/cli.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=24.10.0"
  },
  "scripts": {
    "dev": "tsdown watch src/index.ts",
    "dev:rust": "cd rust && cargo build && cd .. && pnpm run dev",
    "build": "run-s build:rust build:ts",
    "build:ts": "tsdown",
    "build:rust": "cd rust && cargo build --release",
    "build:rust:debug": "cd rust && cargo build",
    "build:napi": "cd rust && cargo build --release",
    "test": "vitest",
    "test:rust": "cd rust && cargo test",
    "type-check": "tsc --noEmit -p tsconfig.lib.json",
    "lint": "eslint --fix .",
    "clean": "rimraf dist && rimraf rust/target",
    "clean:rust": "cd rust && cargo clean",
    "install:rust": "cd rust && cargo install --path .",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "engineStrict": true,
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "inquirer": "^12.9.6",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@truenine/eslint9-config": "^1.0.17",
    "@truenine/tsconfig": "^1.0.7",
    "@tsconfig/node22": "^22.0.2",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.7.0",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
